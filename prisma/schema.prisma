generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db" // Caminho do arquivo de banco de dados SQLite
}

model Filme {
  id       Int     @id @default(autoincrement())
  titulo   String
  diretor  String
  ano      Int
  favorito Boolean @default(false)
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  senha String
}
